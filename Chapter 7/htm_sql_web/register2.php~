<SCRIPT LANGUAGE="JavaScript">
    <!-- Hide code from non-js browsers
    function validateForm()
    {
        formObj = document.sample;
        if ((formObj.user.value == "") ||
            (formObj.pass.value  == "") ||
		(formObj.First.value  == "")||
			(formObj.email.value  == "")||
					(formObj.About.value  == ""))
            
        {
            alert("You have not filled in all the fields.");
            return false;
        }
        else
            return true;
    }





    // end hiding -->
</SCRIPT>

